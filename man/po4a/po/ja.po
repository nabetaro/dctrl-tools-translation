"$ sort-dctrl -kPackage /var/lib/dpkg/status E<gt> stat\n"
"$ sort-dctrl -kPackage Packages E<gt> pkg\n"
"$ join-dctrl -j Package \\e\n"
"  -o 0,1.Version:Old-Version,2.Version:New-Version \\e\n"
"  stat pkg\n"
msgstr ""

